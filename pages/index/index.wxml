<!--index.wxml-->
<view class="container">
    <pin-list>
        <view bindtap='onTeamCardClick'>
            <team-card teamInfo="{{teamInfo}}"/>
        </view>
    </pin-list>
    <pin-list>
        <view bindtap='onEventCardClick'>
            <pin-event-card 
            eventInfo="{{eventInfo}}"
            steps="{{steps}}
            active="{{active}}"/>
        </view>
    </pin-list>
    <pin-list>
        <match-card></match-card>
    </pin-list>
</view>