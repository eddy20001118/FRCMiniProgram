<view class='container'>
    <view class="searchBar">
        <van-search value="{{ search }}" placeholder="请输入搜索关键词" show-action bind:search="onSearch" bind:cancel="onCancel" />
    </view>
    <view class="section">
        <view class="section__title">普通选择器</view>
        <picker bindchange="bindPickerChange" value="{{index}}" range="{{teamYear}}">
            <view class="picker">当前选择：{{teamYear[index]}}</view>
        </picker>
    </view>
</view>