.img {
    width: 30px;
    height: 30px;
}

.circle {
    margin-top: 40px;
    margin-bottom: 15px;
    width: 70px;
    height: 70px;
    background-color: #fff;
    border-radius: 35px;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 3px 5px rgba(26, 26, 26, 0.1);
    box-sizing: border-box;
}

.onClick {
    background-color: #eeeeee;
}
