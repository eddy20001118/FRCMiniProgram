.tank{
    justify-content: center;
    align-items: center;
    flex-direction: column;
    display: flex;
}

.mytext{
    width: 60%;
    color: #424242;
    font-size: 11pt;
    margin-top: 15px;
    margin-bottom: 15px;
    text-align: center;
}

.longtext{
    width: 60%;
    color: #424242;
    font-size: 11pt;
    margin-top: 15px;
    margin-bottom: 15px;
    text-align: left;
}

.search-icon{
    height: 30px;
    width: 30px;
}

.myimage{
    height: 75px;
    width: 75px;
}

.circle {
    width: 75px;
    height: 75px;
    background-color: #3f51b5;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 3px 5px rgba(26, 26, 26, 0.1);
    box-sizing: border-box;
}

.onClick {
    width: 75px;
    height: 75px;
    background-color: #354497;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 3px 5px rgba(26, 26, 26, 0.1);
    box-sizing: border-box;
}