<view>
    <pin-list>
        <view class='list'>
            <view class='list-title'>{{awardTitle}}</view>
            <view wx:for="{{awardTeamList}}" wx:key="{{index}}">
                <view class='list-content'>{{awardTeamList[index].teamNumber}}</view>
                <view class='list-content'>{{awardTeamList[index].teamName}}</view>
            </view>
        </view>
    </pin-list>
</view>