<pin-list>
    <view class='event'>
        <text class='eventTitle'>{{eventInfo.eventTitle}}</text>
        <view class='location-date'>
            <text class='location'>{{eventInfo.eventLocation}}</text>
            <text class='date'>
            {{eventInfo.eventStartDate}} to {{eventInfo.eventEndDate}}, {{eventInfo.eventYear}}
        </text>
        </view>
    </view>
    <slot></slot>
</pin-list>